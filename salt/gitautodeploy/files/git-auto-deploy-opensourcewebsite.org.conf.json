{
  "log-file": "/www/opensourcewebsite.org/logs/git-auto-deploy.log",
  "http-host": "0.0.0.0",
  "http-port": 8005,
  "pid-file": "/www/opensourcewebsite.org/tmp/.gitautodeploy.pid",
  "ssl-cert": "/etc/git-auto-deploy/cert.pem",

